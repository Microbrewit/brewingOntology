###  http://microbrew.it/beer.owl#awardCategory
 mb:awardCategory rdf:type owl:ObjectProperty ;

               rdfs:domain mb:Award .



###  http://microbrew.it/beer.owl#awarded
 mb:awarded rdf:type owl:ObjectProperty ;

         rdfs:range mb:Award ;

         rdfs:domain mb:BeerStyle .



###  http://microbrew.it/beer.owl#awardedAt
 mb:awardedAt rdf:type owl:ObjectProperty ;

           rdfs:domain mb:Award ;

           rdfs:range mb:Festival .



###  http://microbrew.it/beer.owl#brewedBy
 mb:brewedBy rdf:type owl:ObjectProperty ;

          rdfs:domain mb:BeerStyle ;

          rdfs:range mb:Brewery .



###  http://microbrew.it/beer.owl#brews
 mb:brews rdf:type owl:ObjectProperty ;

       rdfs:range mb:BeerStyle ;

       rdfs:domain mb:Brewery ;

       owl:inverseOf mb:brewedBy .



###  http://microbrew.it/beer.owl#locatedIn
 mb:locatedIn rdf:type owl:ObjectProperty ;

           rdfs:domain mb:Brewery ;

           rdfs:range mb:Region .



###  http://microbrew.it/beer.owl#madeFrom
 mb:madeFrom rdf:type owl:ObjectProperty ;

          rdfs:domain mb:BeerStyle ;

          rdfs:range mb:Ingredient .



###  http://microbrew.it/beer.owl#maltedTo
 mb:maltedTo rdf:type owl:ObjectProperty ;

          rdfs:domain mb:Grain ;

          rdfs:range mb:Malt .



###  http://microbrew.it/beer.owl#producedBy
 mb:producedBy rdf:type owl:ObjectProperty .



###  http://microbrew.it/beer.owl#produces
 mb:produces rdf:type owl:ObjectProperty ;

          owl:inverseOf mb:producedBy .



###  http://microbrew.it/beer.owl#sells
 mb:sells rdf:type owl:ObjectProperty ;

       rdfs:range mb:BeerStyle ;

       rdfs:domain mb:Retailer .



###  http://microbrew.it/beer.owl#soldBy
 mb:soldBy rdf:type owl:ObjectProperty ;

        rdfs:domain mb:BeerStyle ;

        rdfs:range mb:Retailer ;

        owl:inverseOf mb:sells ;

        rdfs:subPropertyOf owl:topObjectProperty .



###  http://microbrew.it/beer.owl#sponsors
 mb:sponsors rdf:type owl:ObjectProperty ;

          rdfs:domain mb:Association ;

          rdfs:range mb:Festival .





